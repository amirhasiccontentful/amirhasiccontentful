<apex:page standardController="zqu__Quote__c" standardStylesheets="false" showHeader="false" sidebar="false" extensions="ZuoraQuoteExt" action="{!updateQuote}" >
 <html>
        <head>
            <style type="text/css" media="print">
                @page {
                    @top-center {
                        content: element(header);
                    }
                    @bottom-left {
                        content: element(footer);
                    }
                }
                body {
                }
                div.header {
                    padding: 10px;
                    position: running(header);
                }
                div.footer {
                    display: block;
                    padding: 5px;
                    position: running(footer);
                }
                .pagenumber:before {
                    content: counter(page);
                }
                .pagecount:before {
                    content: counter(pages);
                 }
             </style>
        </head>
        <body>
            <div style="{!IF(showBase != true, '', 'display:none')}">
                <span style="color:red"><i>ERROR: Base platform plan needs to be selected in order to generate a Service Order!</i></span>
            </div>
           <!-- <div style="{!IF(showProjects == true, IF(projectsPrice != platformTotalReccuring, '', 'display:none'), 'display:none')}">
                <span style="color:red"><i>ERROR: Total projects price needs to be equal to Platform price!</i></span>
            </div>-->
            <!--<div class="header" style="{!IF(showBase == true, IF(showProjects == false, '', IF(projectsPrice == platformTotalReccuring, '', 'display:none')), 'display:none')}">
                    <div style="{!IF(zqu__Quote__c.Bill_From__c == 'Contentful GmbH', 'width:60%', 'width:60%')}; float: left;"><img src="{!URLFOR($Resource.AssetsForQuote, 'Logo.png')}" width="170"/></div>
                <div style="float: right;font-size:7.5pt;color:#231f20; line-height: 1.5;font-family:sans-serif;color:#231f20;"><img src="{!URLFOR($Resource.AssetsForQuote, 'Icon_GlobePin.png')}" width="6"/>  <span style="{!IF(zqu__Quote__c.Bill_From__c == 'Contentful GmbH', '', 'display:none')}">Contentful GmbH, {!$Label.CF_GMBH_Address}</span><span style="{!IF(zqu__Quote__c.Bill_From__c == 'Contentful Inc.', '', 'display:none')}">Contentful Inc. 325 9th St, San Francisco, CA 94103, USA</span><br/></div>
            </div>
            <div style="clear: both;"/>
            <div class="content" style="{!IF(showBase == true, IF(showProjects == false, '', IF(projectsPrice == platformTotalReccuring, '', 'display:none')), 'display:none')}">
            -->
            <div class="header" style="{!IF(showBase == true, '', 'display:none')}">
                    <div style="{!IF(zqu__Quote__c.Bill_From__c == 'Contentful GmbH', 'width:60%', 'width:60%')}; float: left;"><img src="{!URLFOR($Resource.AssetsForQuote, 'Logo.png')}" width="170"/></div>
                <div style="float: right;font-size:7.5pt;color:#231f20; line-height: 1.5;font-family:sans-serif;color:#231f20;"><img src="{!URLFOR($Resource.AssetsForQuote, 'Icon_GlobePin.png')}" width="6"/>  <span style="{!IF(zqu__Quote__c.Bill_From__c == 'Contentful GmbH', '', 'display:none')}">Contentful GmbH, {!$Label.CF_GMBH_Address}</span><span style="{!IF(zqu__Quote__c.Bill_From__c == 'Contentful Inc.', '', 'display:none')}">Contentful Inc. 101 Montgomery Street, Suite 2050 San Francisco, CA 94103, USA</span><br/></div>
            </div>
            <div style="clear: both;"/>
            <div class="content" style="{!IF(showBase == true, '', 'display:none')}">

                <br /><br />
                <h1 style="font-size:16pt;font-weight:bold;text-align:center;color: black;font-family:sans-serif;">SERVICE ORDER</h1>
                <br /><br />
                <table cellspacing='0' style="border:0.5px solid #eef2f4;width:100%">
                    <tr>
                        <td style="width:150px; background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">COMPANY</td>
                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!zqu__Quote__c.zqu__Account__r.Name}</td>
                    </tr>
                    <tr>
                        <td style="width:150px; background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">CONTACT</td>
                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!zqu__Quote__c.zqu__SoldToContact__r.Name}</td>
                    </tr>
                    <tr>
                        <td style="width:150px; background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">EMAIL</td>
                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!zqu__Quote__c.zqu__SoldToContact__r.Email}</td>
                    </tr>
                    <tr>
                        <td style="width:150px; background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">QUOTE NAME</td>
                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!zqu__Quote__c.Name}</td>
                    </tr>
                    <tr>
                        <td style="width:150px; background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">SERVICE ORDER NUMBER</td>
                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!zqu__Quote__c.zqu__Number__c}</td>
                    </tr>
                    <tr>
                        <td style="width:150px; background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">VALID UNTIL</td>
                        <td style="padding:5px;font-size:8.5pt;font-family:sans-serif;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">
                            <apex:outputText value="{0, date, yyyy/MM/dd}">
                                <apex:param value="{!zqu__Quote__c.zqu__ValidUntil__c}" /> 
                            </apex:outputText>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:150px; background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">PREPARED BY</td>
                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!zqu__Quote__c.CreatedBy.FirstName}  {!zqu__Quote__c.CreatedBy.LastName} ({!zqu__Quote__c.CreatedBy.Email})</td>
                    </tr>
                    <tr>
                        <td style="width:150px; background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">CURRENCY</td>
                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!zqu__Quote__c.zqu__Currency__c}</td>
                    </tr>
                </table>
                <br/>
                <div style="{!IF(showBase == true, '', 'display:none')}">
                    <h2 style="font-size:8pt;font-weight:bold;color: black;font-family:sans-serif;color:#526171;">PLATFORM</h2>
                    <table cellspacing='0' style="width:100%;border:0.5px solid #eef2f4;">
                        <tr>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">ITEM</th>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">DESCRIPTION</th>
                             <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;width:25%;text-align:right;{!IF(zqu__Quote__c.Show_Prices__c == true, '', 'display:none')}">ANNUAL SUBSCRIPTION FEE</th>
                        </tr>

                        <apex:repeat var="charge" value="{!charges}">
                            <tr  style="{!IF(charge.chargeType == 'platform', '', 'display:none')}">
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;width:20%;">{!charge.name}</td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!charge.description}</td>
                                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;text-align:right;font-family:sans-serif;color:#526171;{!IF(zqu__Quote__c.Show_Prices__c == true, '', 'display:none')}">
                                    <apex:outputText value="{0, Number, {!IF(zqu__Quote__c.zqu__Currency__c == 'USD', '$', '')}#,###{!IF(zqu__Quote__c.zqu__Currency__c == 'EUR', '€', '')}}">
                                        <apex:param value="{!charge.price}" />
                                    </apex:outputText>
                                </td>
                            </tr> 
                        </apex:repeat>
                    </table>  
                    <br />   
                </div>
                
               <div style="{!IF(showBase == true, '', 'display:none')}">
                    <h2 style="font-size:8pt;font-weight:bold;color: black;font-family:sans-serif;color:#526171;">PLATFORM FEATURES</h2>
                    <table cellspacing='0' style="width:100%;border:0.5px solid #eef2f4;">
                        <tr>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">ITEM</th>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">DESCRIPTION</th>
                               
                        </tr>
                        <apex:repeat var="charge" value="{!charges}">
                            <tr  style="{!IF(charge.chargeType == 'addons', IF(charge.name != 'Setup Fee', '', 'display:none'), 'display:none')}">
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!charge.name}</td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!charge.description}</td>
                           
                            </tr> 
                        </apex:repeat>
                    </table>   
                    <br />
                </div>

                <div style="{!IF(showBase == true, '', 'display:none')}">
                    <h2 style="font-size:8pt;font-weight:bold;color: black;font-family:sans-serif;color:#526171;">PROPERTIES</h2>
                    <table cellspacing='0' style="width:100%;border:0.5px solid #eef2f4;">
                        <tr>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">ITEM</th>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right;">UNIT PRICE</th>
                              <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;width:15%;text-align:right;font-family:sans-serif;color:#526171;">QUANTITY</th>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;width:15%;text-align:right;font-family:sans-serif;color:#526171;{!IF(zqu__Quote__c.Show_Prices__c == true, '', 'display:none')};width:25%;">AMOUNT</th>
                        </tr>

                        <apex:repeat var="charge" value="{!charges}">
                            <tr  style="{!IF(charge.chargeType == 'properties', '', 'display:none')}">
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;width:20%;">{!charge.name}</td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right;">
                                <apex:outputText value="{0, Number, {!IF(zqu__Quote__c.zqu__Currency__c == 'USD', '$', '')}#,###{!IF(zqu__Quote__c.zqu__Currency__c == 'EUR', '€', '')}}">
                                        <apex:param value="{!charge.price}" />
                                      </apex:outputText></td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;text-align:right;font-family:sans-serif;color:#526171;">
                                <apex:outputText value="{0, Number, #}">
                                        <apex:param value="{!charge.quantity}" />
                                      </apex:outputText></td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;text-align:right;font-family:sans-serif;color:#526171;{!IF(zqu__Quote__c.Show_Prices__c == true, '', 'display:none')}">
                                    <apex:outputText value="{0, Number, {!IF(zqu__Quote__c.zqu__Currency__c == 'USD', '$', '')}#,###{!IF(zqu__Quote__c.zqu__Currency__c == 'EUR', '€', '')}}">
                                        <apex:param value="{!charge.price*charge.quantity}" />
                                    </apex:outputText>
                                </td>
                            </tr> 
                        </apex:repeat>
                         <tr style="{!IF(zqu__Quote__c.Show_Prices__c == true, '', 'display:none')}">
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;font-weight:bold;">Subtotal</td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right;"></td>
                                 <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right;"></td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right;">
                                    <b><apex:outputText value="{0, Number, {!IF(zqu__Quote__c.zqu__Currency__c == 'USD', '$', '')}#,###{!IF(zqu__Quote__c.zqu__Currency__c == 'EUR', '€', '')}}">
                                        <apex:param value="{!propertiesTotalReccuring}" />
                                    </apex:outputText></b>
                                </td>
                            </tr>
                    </table>  
                    <br />   
                </div>

             
                <div style="{!IF(showBase == true, '', 'display:none')}">
                <h2 style="font-size:8pt;font-weight:bold;color: black;font-family:sans-serif;color:#526171;">INCLUDED LIMITS</h2>
                    <table cellspacing='0' style="width:100%;border:0.5px solid #eef2f4;">
                        <tr>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;width:15%;text-align:left">ITEM</th>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">DESCRIPTION</th>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;width:15%;text-align:right;">INCLUDED</th>
                            <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;width:25%;text-align:right;{!IF(zqu__Quote__c.Show_Prices__c == true, '', 'display:none')}">ADDITIONAL FEES / MONTH</th>

                        </tr>
                            <apex:repeat var="charge" value="{!charges}">
                            <tr  style="{!IF(charge.chargeType == 'limit', '', 'display:none')}">
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">{!charge.name}</td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">{!charge.description}</td>
                                   <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right">          
                                      <span style="{!IF(charge.name == 'Users', '', 'display:none')}"><apex:outputText value="{0, Number, #,###}">
                                        <apex:param value="{!charge.included}" />
                                      </apex:outputText></span>
                                      <span style="{!IF(charge.name == 'API Requests ', '', 'display:none')}"><apex:outputText value="{0, Number, #,###}">
                                        <apex:param value="{!charge.included * 1000}" />
                                      </apex:outputText></span>
                                      <span style="{!IF(charge.name == 'Asset Bandwidth (GB)', '', 'display:none')}"> <apex:outputText value="{0, Number, #,###}">
                                        <apex:param value="{!charge.included}" />
                                    </apex:outputText></span>
                                    </td>

                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right;{!IF(zqu__Quote__c.Show_Prices__c == true, '', 'display:none')}">
                                    <span style="{!IF(charge.name == 'API Requests ', '', 'display:none')}"><apex:outputText value="{0, Number, {!IF(zqu__Quote__c.zqu__Currency__c == 'USD', '$', '')}#,###.###{!IF(zqu__Quote__c.zqu__Currency__c == 'EUR', '€', '')}}">
                                        <apex:param value="{!charge.price}" />
                                    </apex:outputText> per 1,000</span>
                                     <span style="{!IF(charge.name == 'Asset Bandwidth (GB)', '', 'display:none')}"><apex:outputText value="{0, Number, {!IF(zqu__Quote__c.zqu__Currency__c == 'USD', '$', '')}#,###.###{!IF(zqu__Quote__c.zqu__Currency__c == 'EUR', '€', '')}}">
                                        <apex:param value="{!charge.price}" />
                                    </apex:outputText> per 1</span>
                                    <span style="{!IF(charge.name == 'Users', '', 'display:none')}"><apex:outputText value="{0, Number, {!IF(zqu__Quote__c.zqu__Currency__c == 'USD', '$', '')}#,###.###{!IF(zqu__Quote__c.zqu__Currency__c == 'EUR', '€', '')}}">
                                        <apex:param value="{!charge.price}" />
                                    </apex:outputText> per 1</span>
                                </td>
                              
                            </tr> 
                        </apex:repeat>
                        <!--    <tr style="{!IF(numberOfProjects > 0, '', 'display:none')}">
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">Production Spaces</td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left">A combined total of available development and production environments</td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right">{!numberOfProjects}</td>
                                <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right">Included</td>
                            </tr>-->
                    </table>
                     <br/>
                </div>
               
 
                <div style="{!IF(showSLA == true, '', 'display:none')}">
                    <h2 style="font-size:8pt;font-weight:bold;color: black;font-family:sans-serif;color:#526171;">SLA PACKAGE</h2>
                       <table cellspacing='0' style="width:100%;border:0.5px solid #eef2f4;">
                                    <tr>
                                        <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;width:20%;text-align:left;">ITEM</th>
                                        <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:left;">DESCRIPTION</th>
                                        <th style="background-color:#f7f9fa;padding:5px;font-size:8pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;width:25%;text-align:right;{!IF(zqu__Quote__c.Show_Prices__c == true, '', 'display:none')}">ANNUAL SUBSCRIPTION FEE</th>
                                    </tr>
                                   <apex:repeat var="charge" value="{!charges}">                           
                                    <tr style="{!IF(charge.chargeType == 'sla', '', 'display:none')}">
                                            <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!charge.name}</td>
                                            <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">{!charge.description}</td>
                                            <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right;{!IF(zqu__Quote__c.Show_Prices__c == true, '', 'display:none')}">           
                                                <span style="{!IF(charge.price > 0, '', 'display:none')}">
                                                    <apex:outputText value="{0, Number, {!IF(zqu__Quote__c.zqu__Currency__c == 'USD', '$', '')}#,###{!IF(zqu__Quote__c.zqu__Currency__c == 'EUR', '€', '')}}">
                                                        <apex:param value="{!slaTotalReccuring}" />
                                                    </apex:outputText>
                                                </span>
                                                <span style="{!IF(charge.price == 0, '', 'display:none')}">Included</span>
                                             
                                            </td>
                                        </tr>
                                    </apex:repeat>
                                </table>    
                    <br />
                    <table cellspacing='0' style="width:100%;border:0;">
                        <tr>

