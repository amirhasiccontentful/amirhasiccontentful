                                    <tr>
                                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;">Content Delivery API</td>
                                        <td style="padding:5px;font-size:8.5pt;border:0.5px solid #eef2f4;font-family:sans-serif;color:#526171;text-align:right;">{!IF(isSilverSLA != true, IF(isGoldSLA != true, '99.9%', '99.7%'), '99.5%')}</td>
                                    </tr>
